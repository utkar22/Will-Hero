<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.PauseController">
   <children>
      <Button layoutX="390.0" fx:id="continueGame"  layoutY="176.0" onAction="#continueGame" mnemonicParsing="false" prefHeight="59.0" prefWidth="118.0" text="Continue" />
      <Button layoutX="391.0" layoutY="270.0" fx:id="newGame" onAction="#newGame" mnemonicParsing="false" prefHeight="59.0" prefWidth="118.0" text="New Game" />
      <Button layoutX="364.0" layoutY="370.0" fx:id="mainMenu" onAction="#mainMenu" mnemonicParsing="false" prefHeight="59.0" prefWidth="172.0" text="Save&amp;Back To Main Menu" />
      <Button layoutX="71.0" layoutY="63.0" fx:id="exit"  onAction="#exit" mnemonicParsing="false" text="EXIT" />
   </children>
</AnchorPane>
